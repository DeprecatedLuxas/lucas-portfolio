{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "headers": [
    {
      "source": "/:all*(ttf|otf|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "git.luxass.dev"
        }
      ],
      "destination": "https://github.com/luxass",
      "permanent": true
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "gh.luxass.dev"
        }
      ],
      "destination": "https://github.com/luxass",
      "permanent": true
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "git.luxass.com"
        }
      ],
      "destination": "https://github.com/luxass",
      "permanent": true
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "gh.luxass.com"
        }
      ],
      "destination": "https://github.com/luxass",
      "permanent": true
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "linkedin.luxass.dev"
        }
      ],
      "destination": "https://www.linkedin.com/in/lucasnrgaard/",
      "permanent": true
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "linkedin.luxass.com"
        }
      ],
      "destination": "https://www.linkedin.com/in/lucasnrgaard/",
      "permanent": true
    },
    {
      "source": "/view-source",
      "destination": "https://github.com/luxass/luxass.dev"
    },
    {
      "source": "/github",
      "destination": "https://github.com/luxass"
    },
    {
      "source": "/gh",
      "destination": "https://github.com/luxass"
    },
    {
      "source": "/github/:slug*",
      "destination": "https://github.com/luxass/:slug*"
    },
    {
      "source": "/gh/:slug*",
      "destination": "https://github.com/luxass/:slug*"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "latest-vscode-release.luxass.dev"
        }
      ],
      "destination": "https://luxass.dev/latest-vscode-release",
      "permanent": true
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "vscode-releases.luxass.dev"
        }
      ],
      "destination": "https://luxass.dev/vscode-releases",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/art",
      "destination": "/$/art"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "luxass.art"
        }
      ],
      "destination": "/$/art"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "luxass.beer"
        }
      ],
      "destination": "/$/beer"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "luxass.horse"
        }
      ],
      "destination": "/$/horse"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "luxass.lol"
        }
      ],
      "destination": "/$/lol"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "luxass.xyz"
        }
      ],
      "destination": "/$/xyz"
    }
  ],
  "rewrites": [
    {
      "source": "/bump.ts",
      "destination": "https://raw.githubusercontent.com/luxass/luxass/main/scripts/bump.ts"
    },
    {
      "source": "/art",
      "destination": "/$/art"
    },
    {
      "source": "/beer",
      "destination": "/$/beer"
    },
    {
      "source": "/horse",
      "destination": "/$/horse"
    },
    {
      "source": "/lol",
      "destination": "/$/lol"
    },
    {
      "source": "/xyz",
      "destination": "/$/xyz"
    }
  ]
}
