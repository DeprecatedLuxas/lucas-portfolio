{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "headers": [
    {
      "source": "/:all*(ttf|otf|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "git.luxass.dev"
        }
      ],
      "destination": "https://github.com/luxass",
      "permanent": true
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "linkedin.luxass.dev"
        }
      ],
      "destination": "https://www.linkedin.com/in/lucasnrgaard/",
      "permanent": true
    },
    {
      "source": "/view-source",
      "destination": "https://github.com/luxass/luxass.dev"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "import-cost.luxass.dev"
        }
      ],
      "destination": "https://luxass.dev/p/import-cost"
    }
  ]
}
